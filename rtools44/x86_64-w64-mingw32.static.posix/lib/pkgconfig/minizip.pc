prefix=/usr/lib/mxe/usr/x86_64-w64-mingw32.static.posix
exec_prefix=${prefix}
libdir=${prefix}/lib
sharedlibdir=${prefix}/lib
includedir=${prefix}/include/minizip

Name: minizip
Description: Zip manipulation library
Version: 4.0.4

Requires.private: zlib openssl
Libs: -L${libdir} -L${sharedlibdir} -lminizip -lbz2
Libs.private: -lbz2 -llzma -lzstd -lbcrypt
Cflags: -I${includedir}
