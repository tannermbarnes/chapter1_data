prefix=/usr/lib/mxe/usr/x86_64-w64-mingw32.static.posix
includedir=${prefix}/include
libdir=${prefix}/lib

Name: cairo
Description: Multi-platform 2D graphics library
Version: 1.18.0
Requires: zlib, libpng, fontconfig >=  2.2.95, freetype2 >=  9.7.3, pixman-1 >=  0.36.0
Libs: -L${libdir} -lcairo -lm -ldl -lgdi32 -lmsimg32 -ldwrite -ld2d1 -lwindowscodecs
Cflags: -I${includedir}/cairo
Cflags.private: -DCAIRO_WIN32_STATIC_BUILD
Libs.private: -lstdc++
